class Solution(object):
    def triangleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        nums.sort()
        n=len(nums)
        k=n-1
        count=0
        while(k>1):
            i=0
            j=k-1
            while(i<j):
                if nums[i]+nums[j]>nums[k]:
                    count+=j-i
                    j-=1
                else:
                    i+=1
            k-=1
        return count
